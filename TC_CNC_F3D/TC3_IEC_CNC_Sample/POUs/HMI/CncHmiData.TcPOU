//RETURN;

PlcHmiData.NumberOfChannels := gpPform^.nc_config.number_of_channels_r;
FOR nChIdx := 0 TO PlcHmiData.NumberOfChannels - 1 DO
	fbReadChannelData[nChIdx](nChIdx:=nChIdx, pHmiChannel:=ADR(PlcHmiData.Channel[nChIdx]));
END_FOR


]]></ST>
